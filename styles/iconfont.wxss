@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'), url(data:application/font-woff;charset=utf-8;base64,) format('woff'), url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
    padding-left: 20rpx;
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-fankui:before {
    content: "\e613";
}

.icon-shoucang:before {
